{
  "name": "githuborgdeletedtesthenrik-templates",
  "version": "0.0.1",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --watch --configuration=development",
    "start:de": "ng serve --configuration=de",
    "build": "ng build --configuration production --localize --output-path=dist",
    "test": "yarn run test:unit",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "test:watch": "yarn run test --watch",
    "test:unit": "jest",
    "test:cov": "yarn run test:unit --coverage",
    "e2e": "echo There are no e2e tests yet",
    "e2e:ci": "echo There are no e2e tests yet",
    "extract": "ng xi18n --output-path=src/locale",
    "postinstall": "husky install"
  },
  "private": true,
  "prettier": "@dxp/prettier-config",
  "dependencies": {
    "@angular/animations": "14.2.4",
    "@angular/cdk": "14.2.3",
    "@angular/common": "14.2.4",
    "@angular/compiler": "14.2.4",
    "@angular/core": "14.2.4",
    "@angular/localize": "14.2.4",
    "@angular/forms": "14.2.4",
    "@angular/platform-browser": "14.2.4",
    "@angular/platform-browser-dynamic": "14.2.4",
    "@angular/router": "14.2.4",
    "@dxp/ngx-core": "0.1.46",
    "@dynatrace/dtrum-api-types": "1.243.1",
    "@fundamental-ngx/core": "0.36.0",
    "@fundamental-ngx/platform": "0.36.0",
    "@luigi-project/client": "1.25.1",
    "@luigi-project/client-support-angular": "1.25.1",
    "@luigi-project/testing-utilities": "1.25.1",
    "@sap-theming/theming-base-content": "11.1.41",
    "rxjs": "7.5.7",
    "tslib": "2.4.0",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.4",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "14.2.4",
    "@angular/compiler-cli": "14.2.4",
    "@dxp/eslint-config-angular": "git+https://github.tools.sap/dxp/js-config-dxp.git#d021960f785dcaa1275ccbe77911047dd8811db2",
    "@dxp/prettier-config": "git+https://github.tools.sap/dxp/js-config-dxp.git#a3031f00b41c46a5222baedd9c09ac63927e4a9f",
    "@types/jest": "28.1.8",
    "@types/node": "14.18.28",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "eslint": "8.12.0",
    "eslint-plugin-jest": "27.0.1",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-mock-extended": "2.0.7",
    "jest-preset-angular": "12.2.2",
    "lint-staged": "13.0.3",
    "ng-mocks": "14.2.0",
    "prettier": "2.7.1",
    "typescript": "4.6.4"
  },
  "lint-staged": {
    "*.{ts,css,md,html,json,scss}": "prettier --list-different",
    "*.ts": "eslint --cache"
  },
  "packageManager": "yarn@3.0.0"
}
